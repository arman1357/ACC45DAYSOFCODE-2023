#include <iostream>
using namespace std;

int main() {

	int x,y,z,T,i=0;
cout<<"how many tests you want to test\n";
cin>>T;
while(i<T){
    cout<<"enter the no. of 5rs coins you have\n";
    cin>>x;
    cout<<"enter the no. of 10rs coins you have\n";
	cin>>y;
	cout<<"enter the amount of chocalate\n";
	cin>>z;
    int p;
    p=(x*5+y*10)/z;
    cout<<"The maximum no. of chocalates you can buy\n"<<p;
    i++;

}


	return 0;
}
